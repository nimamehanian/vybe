{
  "name": "vybe",
  "version": "0.0.1",
  "description": "start",
  "main": "./src/index.js",
  "author": "Nima Mehanian",
  "license": "MIT",
  "private": true,
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-proposal-class-properties",
      "react-hot-loader/babel"
    ]
  },
  "scripts": {
    "cp-index": "cp ./src/index.html ./dist",
    "cp-favicon": "cp ./src/images/favicon.png ./dist",
    "clean": "rm -rf dist && mkdir dist",
    "copy": "yarn run cp-index && yarn run cp-favicon && webpack --progress",
    "build": "yarn run clean && yarn run copy",
    "start": "yarn run build && webpack-dev-server --inline",
    "test": "jest",
    "test:watch": "yarn run test -- --watch"
  },
  "dependencies": {
    "connected-react-router": "6.0.0",
    "history": "4.7.2",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-hot-loader": "4.6.3",
    "react-redux": "6.0.0",
    "react-router-dom": "4.3.1",
    "redux": "4.0.1",
    "redux-devtools-extension": "2.13.7",
    "redux-observable": "1.0.0",
    "rxjs": "6.3.3",
    "styled-components": "4.1.3"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "7.2.0",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.0.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.4",
    "eslint": "5.11.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "file-loader": "3.0.1",
    "url-loader": "1.1.2",
    "webpack": "4.28.2",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.13"
  }
}
